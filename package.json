{
  "name": "akash-navi",
  "description": "Akash Navi - AI assistant for Akash Network",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "akash",
    "navi",
    "elizaos",
    "ai-assistant"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/src/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "latest",
    "@elizaos/core": "latest",
    "@elizaos/plugin-sql": "latest",
    "@elizaos/plugin-bootstrap": "latest",
    "@elizaos/plugin-akash-chat": "file:./plugins/plugin-akash-chat",
    "@elizaos/plugin-discord": "file:./plugins/plugin-discord",
    "@elizaos/plugin-knowledge": "file:./plugins/plugin-knowledge",
    "@elizaos/plugin-web-search": "file:./plugins/plugin-web-search",
    "dotenv": "^16.4.5",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "tsup": "8.4.0",
    "prettier": "3.5.3",
    "vitest": "3.1.4",
    "@vitest/coverage-v8": "2.1.5"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsup",
    "lint": "prettier --write ./src",
    "test:component": "vitest run",
    "test:e2e": "elizaos test",
    "test": "npm run test:component && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  }
}