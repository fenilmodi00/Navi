{
  "name": "akash-navi",
  "description": "Akash Navi - AI assistant for Akash Network",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "akash",
    "navi",
    "elizaos",
    "ai-assistant"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "plugins/*"
  ],
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@elizaos/cli": "1.0.16",
    "@elizaos/core": "1.0.16",
    "@elizaos/plugin-akash": "workspace:*",
    "@elizaos/plugin-akash-chat": "workspace:*",
    "@elizaos/plugin-bootstrap": "1.0.16",
    "@elizaos/plugin-discord": "1.0.10",
    "@elizaos/plugin-knowledge": "workspace:*",
    "plugin-web-search": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@types/js-yaml": "^4.0.9",
    "@types/pg": "8.15.4",
    "ai": "^4.3.15",
    "axios": "^1.7.9",
    "dotenv": "16.5.0",
    "install": "0.13.0",
    "js-tiktoken": "^1.0.15",
    "pg": "8.16.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/ui": "^2.1.0",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "prettier": "3.5.3",
    "vite": "^5.0.0",
    "vitest": "^2.1.0",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "rollup": "^4.0.0"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos start",
    "test": "elizaos test",
    "test:unit": "vitest run",
    "test:e2e": "elizaos test",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "bun run build:plugins && bun run build:main",
    "build:plugins": "for plugin in plugins/*; do (cd \"$plugin\" && bun run build); done",
    "build:main": "tsup",
    "clean": "rm -rf dist && rm -rf plugins/*/dist",
    "lint": "prettier --check src/**/*.ts plugins/**/*.ts",
    "lint:fix": "prettier --write src/**/*.ts plugins/**/*.ts",
    "typecheck": "tsc --noEmit",
    "install:prod": "bun install --frozen-lockfile --production",
    "docker:build": "docker build -t akash-navi .",
    "docker:build:prod": "docker build -f Dockerfile.production -t akash-navi:prod .",
    "docker:build:optimized": "docker build -f Dockerfile.optimized -t akash-navi:optimized .",
    "docker:build:minimal": "docker build -f Dockerfile.minimal -t akash-navi:minimal .",
    "docker:optimize": "./docker-optimize.sh",
    "docker:run": "docker run -p 3000:3000 --env-file .env akash-navi",
    "docker:run:prod": "docker run -p 3000:3000 --env-file .env akash-navi:prod",
    "docker:run:minimal": "docker run -p 3000:3000 --env-file .env akash-navi:minimal",
    "docker:up": "docker-compose up akash-navi",
    "docker:up:prod": "docker-compose --profile production up akash-navi-prod"
  },
  "resolutions": {
    "zod": "^3.23.8",
    "typescript": "5.8.3",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "@ai-sdk/openai": "^1.3.22",
    "@opentelemetry/api": "^1.9.0",
    "ai": "^4.3.15",
    "@elizaos/core": "^1.0.15",
    "@elizaos/cli": "^1.0.15",
    "@elizaos/plugin-bootstrap": "^1.0.13",
    "js-tiktoken": "^1.0.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
