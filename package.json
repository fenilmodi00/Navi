{
  "name": "navi-akash",
  "description": "A powerful Discord bot for Akash Network powered by elizaOS",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "akash",
    "discord",
    "bot",
    "elizaos",
    "akash-network",
    "cloud",
    "deployment",
    "decentralized"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "plugins/*"
  ],
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@elizaos/adapter-postgres": "^0.25.6-alpha.1",
    "@elizaos/cli": "1.0.8",
    "@elizaos/core": "1.0.8",
    "@elizaos/plugin-akash-chat": "workspace:*",
    "@elizaos/plugin-discord": "^1.0.10",
    "@elizaos/plugin-knowledge": "workspace:*",
    "@elizaos/plugin-sdl-builder": "workspace:*",
    "@elizaos/plugin-sql": "^1.0.8",
    "@elizaos/plugin-web-search": "workspace:*",
    "@types/pg": "^8.15.4",
    "ai": "^4.3.15",
    "dotenv": "^16.4.5",
    "js-tiktoken": "^1.0.15",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "prettier": "3.5.3",
    "vitest": "3.1.4",
    "vite": "^5.0.0",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "rollup": "^4.0.0",
    "@vitest/coverage-v8": "3.1.4"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsup",
    "build:all": "bun run build:plugins && bun run build",
    "build:plugins": "bun run --filter='./plugins/*' build",
    "install:all": "bun install",
    "install:plugins": "bun install --filter='./plugins/*'",
    "lint": "prettier --write ./src",
    "test:component": "vitest run",
    "test:e2e": "elizaos test",
    "test": "npm run test:component && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b165ad83e5f7a21bc1edbd83374ca087e3cd6b33"
}
