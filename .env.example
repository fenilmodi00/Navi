# =============================================================================
# NAVI - AKASH NETWORK SUPPORT AGENT CONFIGURATION
# =============================================================================

# =============================================================================
# REQUIRED API KEYS & AUTHENTICATION
# =============================================================================

# Akash Chat API - Primary LLM provider
# Get your API key from: https://chatapi.akash.network
AKASH_CHAT_API_KEY=your_akash_chat_api_key

# OpenAI API - Routes through Akash Chat API (same as above)
OPENAI_API_KEY=your_akash_chat_api_key

# Tavily API - Web search functionality
# Get your API key from: https://tavily.com
TAVILY_API_KEY=your_tavily_api_key

# Discord Bot Configuration
# Get from: https://discord.com/developers/applications
DISCORD_APPLICATION_ID=your_discord_application_id
DISCORD_API_TOKEN=your_discord_bot_token

# =============================================================================
# AKASH CHAT API CONFIGURATION
# =============================================================================

# API Endpoints
AKASH_CHAT_BASE_URL=https://chatapi.akash.network/api/v1
OPENAI_BASE_URL=https://chatapi.akash.network/api/v1

# Model Configuration
AKASH_CHAT_SMALL_MODEL=Meta-Llama-3-1-8B-Instruct-FP8
AKASH_CHAT_LARGE_MODEL=Meta-Llama-3-1-8B-Instruct-FP8
TEXT_PROVIDER=akash-chat
TEXT_MODEL=Meta-Llama-3-1-8B-Instruct-FP8

# Embedding Configuration
AKASH_CHAT_EMBEDDING_MODEL=BAAI-bge-large-en-v1-5
AKASH_CHAT_EMBEDDING_DIMENSIONS=1024
EMBEDDING_PROVIDER=akash-chat
TEXT_EMBEDDING_MODEL=BAAI-bge-large-en-v1-5
EMBEDDING_DIMENSION=1024

# =============================================================================
# PERFORMANCE & OPTIMIZATION
# =============================================================================

# Request Rate Limits
MAX_CONCURRENT_REQUESTS=200
REQUESTS_PER_MINUTE=1000
TOKENS_PER_MINUTE=750000

# Response Timeouts (milliseconds)
RESPONSE_TIMEOUT=10000
MAX_RESPONSE_TIME=8000
AGENT_RESPONSE_TIMEOUT=8000

# Caching Configuration
ENABLE_DOCUMENT_CACHING=true
CACHE_OPTIMIZATION=true

# =============================================================================
# KNOWLEDGE BASE & DOCUMENT PROCESSING
# =============================================================================

# Knowledge Processing
CTX_KNOWLEDGE_ENABLED=false
LOAD_DOCS_ON_STARTUP=true

# Document Repository Configuration
KNOWLEDGE_PATH=./data/akash-knowledge-base
DOCS_REPO_1_URL=https://github.com/fenilmodi00/akash-knowledge-base.git
DOCS_REPO_1_PATH=./data/akash-knowledge-base
DOCS_REPO_1_BRANCH=main
DOCS_REPO_1_DOCS_PATH=docs-akash

# =============================================================================
# DISCORD BOT BEHAVIOR
# =============================================================================

# Bot Intents & Permissions
DISCORD_INTENTS=Guilds,GuildMessages,MessageContent,GuildMembers

# Response Behavior
DISCORD_ALLOWED_DMS=true
DISCORD_ENABLE_WEB_SEARCH=true
DISCORD_SHOULD_RESPOND_ONLY_TO_MENTIONS=false
DISCORD_SHOULD_IGNORE_BOT_MESSAGES=true
DISCORD_SHOULD_IGNORE_DIRECT_MESSAGES=false

# Channel Configuration (optional - comment out to allow all channels)
# CHANNEL_IDS=your_channel_ids_comma_separated

# Advanced Response Control
SHOULD_RESPOND_BYPASS_TYPES=dm,voice_dm,api,group
SHOULD_RESPOND_BYPASS_SOURCES=discord

# =============================================================================
# SYSTEM CONFIGURATION
# =============================================================================

# Environment & Logging
NODE_ENV=production
LOG_LEVEL=debug

# Server Authentication (optional)
# ELIZA_SERVER_AUTH_TOKEN=your_server_auth_token

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# Supabase Configuration (optional - for enhanced features)
# Get from: https://supabase.com
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# PostgreSQL Database URL
POSTGRES_URL=your_postgresql_connection_string

# =============================================================================
# AKASH NETWORK CONFIGURATION
# =============================================================================

# Wallet Configuration (optional - for advanced features)
AKASH_WALLET_ADDRESS=your_akash_wallet_address
AKASH_MNEMONIC=your_wallet_mnemonic_phrase

# Network Configuration
AKASH_ENV=mainnet
AKASH_NET=https://raw.githubusercontent.com/ovrclk/net/master/mainnet
AKASH_VERSION=v0.20.0
AKASH_CHAIN_ID=akashnet-2
AKASH_NODE=https://rpc.akash.forbole.com:443
RPC_ENDPOINT=https://rpc.akashnet.net:443

# Pricing API Configuration
AKASH_PRICING_API_URL=https://console-api.akash.network/v1/pricing
AKASH_DEFAULT_CPU=1000
AKASH_DEFAULT_MEMORY=1000000000
AKASH_DEFAULT_STORAGE=1000000000

# Optional: Default provider for examples
AKASH_PROVIDER_INFO=akash1ccktptfkvdc67msasmesuy5m7gpc76z75kukpz

# =============================================================================
# OPTIONAL INTEGRATIONS
# =============================================================================

# GitHub Token (optional - for enhanced documentation features)
# Get from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_personal_access_token